<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.atinbo</groupId>
  <artifactId>atinbo-dependencies</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <distributionManagement>
    <repository>
      <id>releases</id>
      <name>Nexus Release</name>
      <url>http://192.168.6.104/repository/maven-releases/</url>
    </repository>
    <snapshotRepository>
      <id>snapshots</id>
      <name>Nexus SNAPSHOTS</name>
      <url>http://192.168.6.104/repository/maven-snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <java.version>1.8</java.version>
    <atinbo-kernel.version>1.0.0-SNAPSHOT</atinbo-kernel.version>
    <flatten-maven-plugin.version>1.1.0</flatten-maven-plugin.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-model</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-common</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-core</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-dal-base</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-dal-mongo</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-dal-redis</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-dal-cache</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-dal-jpa</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-webmvc</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-webflux</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-base</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-security</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>atinbo-generate</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>log-spring-boot-starter</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>swagger-spring-boot-starter</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>dislock-spring-boot-starter</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>rocketmq-spring-boot-starter</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
      <dependency>
        <groupId>com.atinbo</groupId>
        <artifactId>wx-mp-spring-boot-starter</artifactId>
        <version>${atinbo-kernel.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>flatten-maven-plugin</artifactId>
        <version>${flatten-maven-plugin.version}</version>
        <executions>
          <execution>
            <id>flatten</id>
            <phase>process-resources</phase>
            <goals>
              <goal>flatten</goal>
            </goals>
          </execution>
          <execution>
            <id>flatten.clean</id>
            <phase>clean</phase>
            <goals>
              <goal>clean</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <updatePomFile>true</updatePomFile>
          <flattenMode>resolveCiFriendliesOnly</flattenMode>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
